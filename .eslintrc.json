{
    "env": {
        "node": true,
        "commonjs": true,
        "browser": true,
        "es6": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    //   "react/prop-types": "on",
      "react/react-in-jsx-scope": "off"
  }
}
